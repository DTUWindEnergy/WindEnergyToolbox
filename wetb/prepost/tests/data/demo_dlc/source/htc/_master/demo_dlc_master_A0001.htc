  aero tors_ang 1  45.56;
  aero tors_ang 1  59.19;
  aero tors_ang 1  70.87;
  aero tors_ang 1  80.61;
  aero tors_ang 1  84.50;
  aero tors_ang 1  88.40;
  aero tors_ang 1  92.29;
  aero tors_ang 1  96.19;
  aero tors_ang 1  98.13;
