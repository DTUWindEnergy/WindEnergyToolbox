begin simulation;
  time_stop    [time stop];
  solvertype   1 ;    (newmark)
  on_no_convergence continue ;
;  convergence_limits 1E3 1.0 1E-7 ;
  logfile ./logfiles/[Case folder]/[Case id.].log ;
  begin newmark;
    deltat    0.02;
  end newmark;
end simulation;
