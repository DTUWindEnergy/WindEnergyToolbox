  aero_distribution ae_file 1 ;
  ae_sets            1 1 1;
  tiploss_method     1 ;     0=none, 1=prandtl
  dynstall_method    3.0 ;     0=none, 1=stig øye method,2=mhh method
  ; --- Flaps --- ;
  begin dynstall_ateflap ; 
