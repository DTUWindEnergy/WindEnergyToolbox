begin simulation;
  time_stop    1000.0;
  solvertype   1 ;    (newmark)
  on_no_convergence continue ;
  convergence_limits 1E3 1.0 1E-7 ;
  logfile ./log/DTU_10MW_RWT_ver09.log ;
  begin newmark;
    deltat    0.01;  
  end newmark;
end simulation;
