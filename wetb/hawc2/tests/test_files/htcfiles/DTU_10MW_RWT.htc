  mbdy state pos blade1  26 1.0 blade1 # blade 1 tip pos ;
  mbdy state pos blade2  26 1.0 blade2 # blade 2 tip pos ;
  mbdy state pos blade3  26 1.0 blade3 # blade 3 tip pos ;
